   RowBox[{
    RowBox[{"outdir", "=", "\"\</home/pano/projects/sdpb-test/out\>\""}], 
    ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Run", "[", 
     RowBox[{"StringRiffle", "[", 
      RowBox[{"{", "\[IndentingNewLine]", 
       RowBox[{"\"\<rm -rf\>\"", ",", "\[IndentingNewLine]", 
        RowBox[{"FileNameJoin", "[", 
         RowBox[{"sdpdir", ",", "\"\<*\>\""}], "]"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"sdpdir", "<>", "\"\<.ck\>\""}], ",", "\[IndentingNewLine]", 
        RowBox[{"FileNameJoin", "[", 
         RowBox[{"outdir", ",", "\"\<*\>\""}], "]"}]}], "\[IndentingNewLine]",
        "}"}], "]"}], "]"}], ";"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Maximum", " ", "order", " ", "of", " ", "derivatives", " ", "in", " ", 
     "the", " ", "linear", " ", "functional", " ", "a"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"P", "=", "25"}], ";"}], " ", "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Central", " ", "charge"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"c", " ", "=", " ", "12"}], ";"}], " ", "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Gap", " ", "energy"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"E0", "=", 
     FractionBox["1", "2"]}], ";"}], " ", "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Polynomial", " ", "generator", " ", "function"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"poly", "[", 
      RowBox[{"n_", ",", "E0_", ",", "x_"}], "]"}], ":=", 
     RowBox[{"Sum", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"StirlingS2", "[", 
         RowBox[{"n", ",", "k"}], "]"}], 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           RowBox[{"-", "2"}], "\[Pi]", " ", 
           RowBox[{"(", 
            RowBox[{"x", "+", "E0"}], ")"}]}], ")"}], "k"]}], ",", 
       RowBox[{"{", 
        RowBox[{"k", ",", "1", ",", "n"}], "}"}]}], "]"}]}], ";"}], " ", 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Construct", " ", "a", " ", "module", " ", "that", " ", "does", " ", 
     "this", " ", "whole", " ", "sdpb", " ", "definition", " ", "thingy"}], 
    "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"solve", "[", 
     RowBox[{
      RowBox[{"gapEnergy_", ":", "E0"}], ",", 
      RowBox[{"centralCharge_", ":", "c"}], ",", " ", 
      RowBox[{"order_", ":", "P"}], ",", " ", 
      RowBox[{"precision_", ":", "prec"}]}], "]"}], ":=", 
    "\[IndentingNewLine]", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"E0", "=", "gapEnergy"}], ",", "\[IndentingNewLine]", 
        RowBox[{"c", "=", "centralCharge"}], ",", "\[IndentingNewLine]", 
        RowBox[{"NN", "=", 
         RowBox[{"Floor", "[", 
          RowBox[{"P", "/", "2"}], "]"}]}], ",", "\[IndentingNewLine]", 
        RowBox[{"prec", "=", "precision"}], ",", "\[IndentingNewLine]", 
        "objective", ",", "\[IndentingNewLine]", "normalization", ",", 
        "\[IndentingNewLine]", "matrices"}], "\[IndentingNewLine]", "}"}], ",",
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{"Objective", " ", "vector"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"objective", " ", "=", " ", 
        RowBox[{
         RowBox[{"{", "0", "}"}], "~", "Join", "~", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{"poly", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"2", "#"}], "+", "1"}], ",", "0", ",", 
              RowBox[{"-", 
               FractionBox["c", "12"]}]}], "]"}], "&"}], "/@", 
           RowBox[{"Range", "[", 
            RowBox[{"0", ",", "NN"}], "]"}]}], ")"}]}]}], ";", 
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{"Normalization", " ", "vector"}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"normalization", " ", "=", " ", 
        RowBox[{
         RowBox[{"{", "1", "}"}], "~", "Join", "~", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"Range", "[", 
            RowBox[{"0", ",", "NN"}], "]"}], "*", "0"}], ")"}]}]}], ";", 
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{"Polynomial", " ", "list"}], "*)"}], "\[IndentingNewLine]", 
       RowBox[{"matrices", "=", 
        RowBox[{"{", "\[IndentingNewLine]", 
         RowBox[{"PositiveMatrixWithPrefactor", "[", 
          RowBox[{"<|", "\[IndentingNewLine]", 
           RowBox[{"\"\<polynomials\>\"", "->", 
            RowBox[{"{", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"{", "0", "}"}], "~", "Join", "~", 
               RowBox[{"(", 
                RowBox[{
                 RowBox[{
                  RowBox[{"Expand", "[", 
                   RowBox[{
                    RowBox[{"poly", "[", 
                    RowBox[{
                    RowBox[{
                    RowBox[{"2", "#"}], "+", "1"}], ",", "E0", ",", "x"}], 
                    "]"}], ",", "x"}], "]"}], "&"}], "/@", 
                 RowBox[{"Range", "[", 
                  RowBox[{"0", ",", "NN"}], "]"}]}], ")"}]}], "}"}], "}"}]}], 
           "\[IndentingNewLine]", "|>"}], "]"}], "\[IndentingNewLine]", 
         "}"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{"Define", " ", "SDP"}], "*)"}], "\[IndentingNewLine]", 
       RowBox[{"WritePmpJson", "[", 
        RowBox[{"file", ",", " ", 
         RowBox[{"SDP", "[", 
          RowBox[{"objective", ",", "normalization", ",", "matrices"}], "]"}],
          ",", " ", "prec"}], "]"}], ";", "\[IndentingNewLine]", 
       "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{
        "Run", " ", "the", " ", "magical", " ", "code", " ", "to", " ", 
         "solve"}], "*)"}], "\[IndentingNewLine]", 
       RowBox[{"Run", "[", 
        RowBox[{"StringRiffle", "[", 
         RowBox[{"{", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"FileNameJoin", "[", 
            RowBox[{"{", 
             RowBox[{"executablesPath", ",", "\"\<pmp2sdp\>\""}], "}"}], 
            "]"}], ",", "\[IndentingNewLine]", 
           RowBox[{"ToString", "[", "prec", "]"}], ",", " ", "file", ",", " ",
            "sdpdir"}], "\[IndentingNewLine]", "}"}], "]"}], "]"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"Run", "[", 
        RowBox[{"StringRiffle", "[", 
         RowBox[{"{", "\[IndentingNewLine]", 
          RowBox[{"\"\<mpirun\>\"", ",", "\[IndentingNewLine]", 
           RowBox[{"FileNameJoin", "[", 
            RowBox[{"{", 
             RowBox[{"executablesPath", ",", "\"\<sdpb\>\""}], "}"}], "]"}], ",",
            "\[IndentingNewLine]", "\"\<-s\>\"", ",", " ", "sdpdir", ",", 
           " ", "\"\<-o\>\"", ",", " ", "outdir", ",", 
           "\[IndentingNewLine]", "\"\<--precision\>\"", ",", " ", 
           RowBox[{"ToString", "[", "prec", "]"}], ",", 
           "\[IndentingNewLine]", "\"\<--noFinalCheckpoint\>\""}], 
          "\[IndentingNewLine]", "}"}], "]"}], "]"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"fullResult", " ", "=", " ", 
        RowBox[{"Import", "[", 
         RowBox[{
          RowBox[{"FileNameJoin", "[", 
           RowBox[{"{", 
            RowBox[{"outdir", ",", "\"\<out.txt\>\""}], "}"}], "]"}], ",", 
          " ", "\"\<String\>\""}], "]"}]}]}]}], "\[IndentingNewLine]", 
     "]"}]}]}]}]], "Input",
