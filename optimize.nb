Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{"Precision", " ", "and", " ", "directories"}], "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"precision", " ", "=", " ", "600"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
    "file", " ", "=", "\"\</home/pano/projects/sdpb-test/pmp.json\>\""}], 
